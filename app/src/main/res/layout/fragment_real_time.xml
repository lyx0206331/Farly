                android:text="@string/reach_window"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_acc_limit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/acc_limit"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_real_height"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/real_height"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_normal_rsp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
