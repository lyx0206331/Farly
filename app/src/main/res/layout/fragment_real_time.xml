                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_all_null_distance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/all_null_distance"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
