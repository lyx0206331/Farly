                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/real_pos"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_set_pos_x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/set_pos"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_set_velo_x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/set_velo"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_pos_diff_x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/pos_diff"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/y_info"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_real_pos_y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/real_pos"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_set_pos_y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/set_pos"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_set_velo_y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/set_velo"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/tv_pos_diff_y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/realtime_item_margin_left"
                android:text="@string/pos_diff"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/z_info"
                android:textSize="18sp" />

            <TextView
